{
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "http://ec2-3-107-23-86.ap-southeast-2.compute.amazonaws.com:8080/api/:path*",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Methods": "GET, POST, PUT, DELETE, OPTIONS",
        "Access-Control-Allow-Headers": "Content-Type, Authorization"
      }
    },
    {
      "source": "/uploads/:path*",
      "destination": "http://ec2-3-107-23-86.ap-southeast-2.compute.amazonaws.com:8080/uploads/:path*"
    }
  ],
  "headers": [
    {
      "source": "/api/:path*",
      "headers": [
        { "key": "Access-Control-Allow-Origin", "value": "*" },
        { "key": "Access-Control-Allow-Methods", "value": "GET,POST,PUT,DELETE,OPTIONS" },
        { "key": "Access-Control-Allow-Headers", "value": "Content-Type, Authorization" }
      ]
    }
  ],
  "routes": [
    {
      "src": "/register",
      "dest": "/index.html",
      "status": 200
    },
    {
      "src": "/.*",
      "dest": "/index.html",
      "status": 200
    }
  ],
  "redirects": [
    {
      "source": "/:path*",
      "destination": "https://it-342-skill-match.vercel.app/:path*",
      "permanent": true
    }
  ]
}